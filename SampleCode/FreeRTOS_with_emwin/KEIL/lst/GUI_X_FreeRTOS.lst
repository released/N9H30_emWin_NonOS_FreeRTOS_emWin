N/*********************************************************************
N*
N*      Multitasking:
N*
N*                 GUI_X_InitOS()
N*                 GUI_X_GetTaskId()
N*                 GUI_X_Lock()
N*                 GUI_X_Unlock()
N*
N* Note:
N*   The following routines are required only if emWin is used in a
N*   true multi task environment, which means you have more than one
N*   thread using the emWin API.
N*   In this case the
N*                       #define GUI_OS 1
N*  needs to be in GUIConf.h
N*/
Nvoid GUI_X_InitOS(void)    { _Semaphore = xSemaphoreCreateMutex(); }
Xvoid GUI_X_InitOS(void)    { _Semaphore = xQueueCreateMutex( ( ( uint8_t ) 1U ) ); }
Nvoid GUI_X_Unlock(void)    { xSemaphoreGive(_Semaphore); }
Xvoid GUI_X_Unlock(void)    { xQueueGenericSend( ( QueueHandle_t ) ( _Semaphore ), 0, ( ( TickType_t ) 0U ), ( ( BaseType_t ) 0 ) ); }
Nvoid GUI_X_Lock(void)      { xSemaphoreTake(_Semaphore, portMAX_DELAY);  }
Xvoid GUI_X_Lock(void)      { xQueueGenericReceive( ( QueueHandle_t ) ( _Semaphore ), 0, ( ( TickType_t ) 0xffffffffUL ), ( ( BaseType_t ) 0 ) );  }
