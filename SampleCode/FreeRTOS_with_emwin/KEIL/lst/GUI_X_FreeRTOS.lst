N/*********************************************************************
N*
N*      Timing:
N*                 GUI_GetTime()
N*                 GUI_Delay(int)
N
N  Some timing dependent routines require a GetTime
N  and delay funtion. Default time unit (tick), normally is
N  1 ms.
N*/
N
Nint GUI_X_GetTime(void) {
N  return xTaskGetTickCount() * portTICK_PERIOD_MS;
X  return xTaskGetTickCount() * ( ( TickType_t ) 1000 / ( ( TickType_t ) 1000 ) );
N}
N
Nvoid GUI_X_Delay(int Period) {
N  vTaskDelay(Period / portTICK_PERIOD_MS);
X  vTaskDelay(Period / ( ( TickType_t ) 1000 / ( ( TickType_t ) 1000 ) ));
N}
N
N/*********************************************************************
N*
N*       GUI_X_ExecIdle()
N*
N*/
Nvoid GUI_X_ExecIdle(void) {
N  vTaskDelay(1 / portTICK_PERIOD_MS);
X  vTaskDelay(1 / ( ( TickType_t ) 1000 / ( ( TickType_t ) 1000 ) ));
N}
