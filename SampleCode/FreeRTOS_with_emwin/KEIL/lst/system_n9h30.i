int sysSetCachePages(unsigned int vaddr, int size, int cache_flag)
{
    int i, cnt, table_num, page_num, cache_mode;
    unsigned volatile int baseaddr, temp;
    volatile _CTable *PageTabPtr;

    if (vaddr & 0x80000000)
        PageTabPtr = (_CTable *) _mmuCoarsePageTable_NonCache; 
    else
        PageTabPtr = (_CTable *) _mmuCoarsePageTable;   

    if (sysGetCacheState() == (1))
        PageTabPtr = (_CTable *) ((unsigned int)PageTabPtr | 0x80000000); 

    vaddr &= 0x7FFFFFFF;    
    
    if ((vaddr + size) > (64 << 20)) return -1;

    if (vaddr & 0xFFF)  return -1;   
    if (size % 4096)    return -1;   

     
    cnt = size / 4096;

    if (cache_flag == 0)  
        cache_mode = 0x0C;
    else if (cache_flag == 1)  
        cache_mode = 0x08;
    else
        cache_mode = 0;  

    for (i=0; i<cnt; i++) {
        baseaddr = vaddr + i * 4096;
        table_num = baseaddr / 0x100000;
        page_num =  (baseaddr & 0xFF000) >> 12;  

        temp = (*(PageTabPtr+table_num)).page[page_num] & 0xFFFFFFF3;
        temp |= cache_mode;  
        (*(PageTabPtr+table_num)).page[page_num] = temp;
    }

    

    return 0;

}  



