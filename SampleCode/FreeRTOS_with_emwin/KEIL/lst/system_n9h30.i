    if ((vaddr + size) > (64 << 20)) return -1;
    if (vaddr & 0xFFFFF)    return -1;   
    if (size % 4096)        return -1;   

     
    cnt = size / 4096;

    if (cache_flag == 0)  
        cache_mode = 0x0C;
    else if (cache_flag == 1)  
        cache_mode = 0x08;
    else
        cache_mode = 0;  


    if (rev_flag == 0)
        phy_base_addr = phy_addr;
