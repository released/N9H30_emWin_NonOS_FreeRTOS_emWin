    return _sys_IsCacheOn;
}


INT32 sysGetCacheMode()
{
    return _sys_CacheMode;
}


INT32 _sysLockCode(UINT32 addr, INT32 size)
{
    int i, cnt, temp;

    __asm {
         
        MRC p15, 0, temp, c9, c0, 1 ;
        ORR temp, temp, 0x07 ;
        MCR p15, 0, temp, c9, c0, 1 ;
    }

    if (size % 16)  cnt = (size/16) + 1;
    else            cnt = size / 16;

    for (i=0; i<cnt; i++) {
        __asm {
            MCR p15, 0, addr, c7, c13, 1;
        }

        addr += 16;
    }


    __asm {
         
        MRC p15, 0, temp, c9, c0, 1 ;
        BIC temp, temp, 0x07 ;
        ORR temp, temp, 0x08 ;
        MCR p15, 0, temp, c9, c0, 1 ;
    }

    return 0;

}


INT32 _sysUnLockCode()
{
    int temp;

     
    __asm {
        MRC p15, 0, temp, c9, c0, 1;
        BIC temp, temp, 0x08 ;
        MCR p15, 0, temp, c9, c0, 1;

    }

    return 0;
}










 
INT32 sysEnableCache(UINT32 uCacheOpMode)
{
    sysInitMMUTable(uCacheOpMode);
    _sys_IsCacheOn = (1);
    _sys_CacheMode = uCacheOpMode;

    return 0;
}





 
void sysDisableCache(void)
{
    int temp;

    sys_flush_and_clean_dcache();
    __asm {
         
        MOV temp, 0x0
        MCR p15, 0, temp, c7, c5, 0  
        MCR p15, 0, temp, c7, c6, 0  
        MCR p15, 0, temp, c7, c10,4  

         
        MRC p15, 0, temp, c1, c0, 0      
        BIC temp, temp, 0x01
        MCR p15, 0, temp, c1, c0, 0      
    }
    _sys_IsCacheOn = (0);
    _sys_CacheMode = -1;

}

 
