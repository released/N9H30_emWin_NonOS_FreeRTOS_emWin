INT32 GPIO_OpenBit(GPIO_PORT port, UINT32 bit, GPIO_DIR direction, GPIO_PULL pull)
{
    UINT32 reg;
    UINT32 mask;
    INT32 portIndex;
    INT32 offset;

    *((volatile unsigned int *)((0xB0000200+0x18))) = (*((volatile unsigned int *)((0xB0000200+0x18)))) | (1<<3); 
    offset = (INT32)port;
    portIndex = (port / 0x040);
    mask = (UINT32)bit;
    
    if((gpioCfg.bitBusyFlag[portIndex]&mask) != 0)
    {
        return -3;
    }

    gpioCfg.bitBusyFlag[portIndex] = gpioCfg.bitBusyFlag[portIndex] | mask;
    
    reg = (*((volatile unsigned int *)((0xB8003000+0x000) + offset)));
    reg = reg & (~mask);

    if(direction == DIR_OUTPUT)
    {
        reg = reg | mask;
    }
    
    *((volatile unsigned int *)((0xB8003000+0x000) + offset)) = reg;

    reg = (*((volatile unsigned int *)((0xB8003000+0x020) + offset)));
    reg = reg & (~mask);

    if(pull == PULL_UP)
    {
        reg = reg | mask;
    }

    *((volatile unsigned int *)((0xB8003000+0x020) + offset)) = reg;

    return 0;
}

 









 
