N/* Interrupt Service Routine for H/W JPEG CODEC */
Nvoid jpegISR(void)
N{
N    UINT32 u32interruptStatus;
N
N    /* Get the interrupt status */
N    u32interruptStatus = JPEG_GET_INT_STATUS();
X    u32interruptStatus = ((*((volatile unsigned int *)((0xB000A000+0x2C)))) & 0x010000FF);
N
N    /* It's Header Decode Complete Interrupt */
N    if(u32interruptStatus & DHE_INTS) {
X    if(u32interruptStatus & (0x00000040)) {
N        UINT32 u32YuvFormat;
N        UINT16 u16Height,u16Width,UVHeight,UVWidth;
N        UINT16 u16WidthTmp, u16HeightTmp;
N        /* Get the JPEG format */
N        u32YuvFormat = JPEG_DEC_GET_DECODED_IMAGE_FORMAT();
X        u32YuvFormat = ((*((volatile unsigned int *)((0xB000A000+0x08)))) & ((0xFFFFFFFFUL >> (31 - 10)) & (0xFFFFFFFFUL >>8 << 8)));
N
N        /* Get the decoded image dimension */
N        jpegGetDecodedDimension(&u16Height,&u16Width);
N
N        jpegInfo.jpeg_width = u16Width;
N        jpegInfo.jpeg_height = u16Height;
N        jpegInfo.yuvformat = u32YuvFormat;
N
N        if(pfnJpegHeaderDecode!= NULL) {
X        if(pfnJpegHeaderDecode!= 0) {
N            if(!pfnJpegHeaderDecode()) {
N                jpegInit();
N                g_bWait = FALSE;
X                g_bWait = (0);
N                return;
N            }
N        }
N
N        if(g_bScale) {
N            UINT16 u16RatioH,u16RatioW;
N
N            if(jpegCalScalingFactor(
N                   g_u32OpMode,         //Up / Down Scaling
N                   u16Height,               //Original Height
N                   u16Width,                //Original Width
N                   g_u32ScaleHeight,            //Scaled Height
N                   g_u32ScaleWidth,         //Scaled Width
N                   &u16RatioH,              //Horizontal Ratio
N                   &u16RatioW               //Vertical Ratio
N               ) != E_SUCCESS) {
X               ) != 1) {
N                g_bWait = FALSE;
X                g_bWait = (0);
N                g_jpegError = TRUE;
X                g_jpegError = (1);
N            } else {
N                jpegSetScalingFactor(g_u32OpMode,u16RatioH,u16RatioW);
N                u16Width =  g_u32ScaleWidth;
N                u16Height = g_u32ScaleHeight;
N            }
N        } else {
N
N            if(u32YuvFormat == JPEG_DEC_YUV411) {
X            if(u32YuvFormat == 0x300) {
N                /* 32-pixel alignment for YUV411 raw data */
N                if(u16Width % 32)
N                    u16Width = (u16Width & 0xFFFFFFE0) + 32;
N            } else if((u32YuvFormat == JPEG_DEC_YUV444) || (u32YuvFormat == JPEG_DEC_YUV422T)) {
X            } else if((u32YuvFormat == 0x200) || (u32YuvFormat == 0x500)) {
N                /* 8-pixel alignment for YUV444 raw data */
N                if(u16Width % 8)
N                    u16Width = (u16Width & 0xFFFFFFF8) + 8;
N            } else {
N                /* 16-pixel alignment for YUV422 or YUV420 raw data */
N                if(u16Width % 16)
N                    u16Width = (u16Width & 0xFFFFFFF0) + 16;
N            }
N        }
N
N        if(g_u32Stride >= u16Width) {
N            jpegInfo.stride = g_u32Stride;
N            g_u32Stride = g_u32Stride - u16Width;
N            JPEG_SET_YSTRIDE(g_u32Stride);
X            *((volatile unsigned int *)((0xB000A000+0x94))) = g_u32Stride;
N            u16Width = jpegInfo.stride;
N        } else {
N            g_u32Stride = 0;
N            JPEG_SET_YSTRIDE(0);
X            *((volatile unsigned int *)((0xB000A000+0x94))) = 0;
N            jpegInfo.stride = 0;
N        }
N
N        if(g_u32OutputFormat == JPEG_DEC_PRIMARY_PLANAR_YUV || g_u32OutputFormat ==JPEG_DEC_THUMBNAIL_PLANAR_YUV) {
X        if(g_u32OutputFormat == 0x8021 || g_u32OutputFormat ==0x8031) {
N            if(g_u32WindowDec) {
N                u16WidthTmp = u16Width;
N                u16HeightTmp = u16Height;
N
N                u16Width = g_u32windowSizeX;
N                u16Height = g_u32windowSizeY;
N            }
N
N            if(u32YuvFormat == JPEG_DEC_YUV411) {
X            if(u32YuvFormat == 0x300) {
N                /* For YUV411 raw data */
N                UVWidth = u16Width/4;
N            } else if((u32YuvFormat == JPEG_DEC_YUV444) || (u32YuvFormat == JPEG_DEC_YUV422T)) {
X            } else if((u32YuvFormat == 0x200) || (u32YuvFormat == 0x500)) {
N                /* For YUV444 raw data */
N                UVWidth = u16Width;
N            }
N            /* Set the U-component and V-componente width for YUV422 or YUV420 raw data */
N            else if(u16Width % 2)
N                UVWidth = u16Width / 2 + 1;
N            else
N                UVWidth = u16Width / 2;
N
N
N            /* Sets the height of U and V for YUV420 image */
N            if(u32YuvFormat == JPEG_DEC_YUV420) {
X            if(u32YuvFormat == 0x000) {
N                /* 16-pixel alignment for YUV422 or YUV420 raw data */
N                if(u16Height % 16)
N                    u16Height = (u16Height & 0xFFFFFFF0) + 16;
N                UVHeight = u16Height / 2;
N            } else if(u32YuvFormat == JPEG_DEC_YUV422) {
X            } else if(u32YuvFormat == 0x100) {
N                /* 8-pixel alignment for YUV444 raw data */
N                if(u16Height % 8)
N                    u16Height = (u16Height & 0xFFFFFFF8) + 8;
N                UVHeight = u16Height;
N            } else if(u32YuvFormat == JPEG_DEC_YUV444) {
X            } else if(u32YuvFormat == 0x200) {
N                /* 8-pixel alignment for YUV444 raw data */
N                if(u16Height % 8)
N                    u16Height = (u16Height & 0xFFFFFFF8) + 8;
N                UVHeight = u16Height;
N            } else if(u32YuvFormat == JPEG_DEC_YUV411) {
X            } else if(u32YuvFormat == 0x300) {
N                /* 8-pixel alignment for YUV411 raw data */
N                if(u16Height % 8)
N                    u16Height = (u16Height & 0xFFFFFFF8) + 8;
N                UVHeight = u16Height;
N            } else if(u32YuvFormat == JPEG_DEC_YUV422T) {
X            } else if(u32YuvFormat == 0x500) {
N                /* 16-pixel alignment for YUV422 or YUV420 raw data */
N                if(u16Height % 16)
N                    u16Height = (u16Height & 0xFFFFFFF0) + 16;
N                UVHeight = u16Height / 2;
N            } else {
N                /* 8-pixel alignment for raw data */
N                if(u16Height % 8)
