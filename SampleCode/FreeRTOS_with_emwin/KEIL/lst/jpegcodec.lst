N        /* Get the image dimension */
N        jpegGetDimension(&imageHeight,&imageWidth);
N
N        if(g_u32Stride!=0) {
N            imageWidth = imageWidth - g_u32Stride;
N        }
N
N        jpegInfo.width = imageWidth;
N        jpegInfo.height = imageHeight;
N
N        /* Clear interrupt status */
N        JPEG_CLEAR_INT(DEC_INTS);
X        *((volatile unsigned int *)((0xB000A000+0x2C))) = ((*((volatile unsigned int *)((0xB000A000+0x2C)))) & ~0xFF) | (0x00000004);
