N    u32JPGDiv = 0;
N
N    if(u32JPGSource > 75000000)
N    {
N        if(u32JPGSource % 75000000)
N        {
N            u32JPGDiv = (u32JPGSource / 75000000);
N        }
N        else
N            u32JPGDiv = (u32JPGSource / 75000000) - 1;
N    }
N    outp32(REG_CLK_DIVCTL3, (inp32(REG_CLK_DIVCTL3) & ~(0xf0000000)) | ((u32JPGDiv & 0xf) << 28));
X    *((volatile unsigned int *)((0xB0000200+0x2C))) = ((*((volatile unsigned int *)((0xB0000200+0x2C)))) & ~(0xf0000000)) | ((u32JPGDiv & 0xf) << 28);
