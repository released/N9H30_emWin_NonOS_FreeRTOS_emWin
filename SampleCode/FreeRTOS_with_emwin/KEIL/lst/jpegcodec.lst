N                UVHeight = u16Height;
N            }
N
N            JPEG_SET_UADDR(JPEG_GET_YADDR() + u16Width * u16Height);
X            *((volatile unsigned int *)((0xB000A000+0x80))) = (*((volatile unsigned int *)((0xB000A000+0x7C)))) + u16Width * u16Height;
N            JPEG_SET_VADDR(JPEG_GET_UADDR() + UVWidth * UVHeight);
X            *((volatile unsigned int *)((0xB000A000+0x84))) = (*((volatile unsigned int *)((0xB000A000+0x80)))) + UVWidth * UVHeight;
N
N            if(u32YuvFormat == JPEG_DEC_GRAY)
X            if(u32YuvFormat == 0x400)
N                jpegInfo.image_size[0] = u16Width * u16Height;
N            else
N                jpegInfo.image_size[0] = u16Width * u16Height + 2* UVWidth * UVHeight;
N
N            if(g_u32WindowDec) {
N                u16Width = u16WidthTmp;
N                u16Height = u16HeightTmp;
N            }
N        } else {
N            if(g_u32WindowDec) {
N                u16WidthTmp = u16Width;
N                u16HeightTmp = u16Height;
N
N                u16Width = g_u32windowSizeX;
N                u16Height = g_u32windowSizeY;
N            }
N
N            if(jpegInfo.stride)
N                jpegInfo.image_size[0] = jpegInfo.stride * u16Height;
N            else
N                jpegInfo.image_size[0] =  u16Width * u16Height;
N
N            if (g_u32OutputFormat == JPEG_DEC_PRIMARY_PACKET_RGB888)
X            if (g_u32OutputFormat == 0x14021)
N                jpegInfo.image_size[0] = 4 * jpegInfo.image_size[0];
N            else
N                jpegInfo.image_size[0] = 2 * jpegInfo.image_size[0];
N
N            if(g_u32WindowDec) {
N                u16Width = u16WidthTmp;
N                u16Height = u16HeightTmp;
N            }
N
N        }
N
N        /* Set the image dimension */
N        jpegSetDimension(u16Height, u16Width);
N
N
N        outp32(JITCR, inp32(JITCR) | Dec_Scatter_Gather);
X        *((volatile unsigned int *)((0xB000A000+0x08))) = (*((volatile unsigned int *)((0xB000A000+0x08)))) | (0x00040000);
N
N        /* Clear interrupt status */
N        JPEG_CLEAR_INT(DHE_INTS);
X        *((volatile unsigned int *)((0xB000A000+0x2C))) = ((*((volatile unsigned int *)((0xB000A000+0x2C)))) & ~0xFF) | (0x00000040);
N    }
N    /* It's Decode Output Wait Interrupt */
N    else if(u32interruptStatus & JPG_DOW_INTS) {
X    else if(u32interruptStatus & (0x01000000)) {
N        if(pfnJpegDecOutputWait!= NULL) {
X        if(pfnJpegDecOutputWait!= 0) {
N            pfnJpegDecOutputWait(inp32(JYADDR0) , inp32(JDOWFBS));
X            pfnJpegDecOutputWait((*((volatile unsigned int *)((0xB000A000+0x7C)))) , (*((volatile unsigned int *)((0xB000A000+0x3c)))));
