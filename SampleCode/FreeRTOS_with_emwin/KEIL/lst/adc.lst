N/**
N * @brief       The ChangeChannel function of ADC device library
N *
N * @param       channel    Select ADC input for normal mode.Including:
N *                         - \ref AIN0
N *                         - \ref AIN1
N *                         - \ref AIN2
N *                         - \ref AIN3
N *                         - \ref AIN4
N *                         - \ref AIN5
N *                         - \ref AIN6
N *                         - \ref AIN7
N *
N * @retval      <0         Wrong argument
N * @retval      0          Success
N *
N * @details     This function is used to set channel for normal mode.
N */
NINT adcChangeChannel(int channel)
N{
N    UINT32 reg;
N    if ((channel>>3) < 0 || (channel>>3) > 7) {
N        return ADC_ERR_ARGS;
X        return 1;
N    }
N    reg = inpw(REG_ADC_CONF);
X    reg = (*((volatile unsigned int *)((0xB800A000+0x004))));
N    reg = (reg & ~ADC_CONF_CHSEL_Msk) | channel;
X    reg = (reg & ~(7<<3)) | channel;
N    outpw(REG_ADC_CONF,reg);
X    *((volatile unsigned int *)((0xB800A000+0x004))) = reg;
N    return Successful;
X    return 0;
N}
