N/**
N * @brief       Open ADC2 Function.
N *
N * @param[in]   freqKhz  The ADC engine clock. It should be 1000Khz~3000Khz
N *
N * @retval      <0              Fail
N * @retval      0               Success
N *
N * @details     This function is used to open adc function.
N */
NINT adcOpen2(uint32_t freqKhz)
N{
N    uint32_t div;
N    /* Enable ADC engine clock */
N    outpw(REG_CLK_PCLKEN1, 1<<24);
X    *((volatile unsigned int *)((0xB0000200+0x1C))) = 1<<24;
N
N    /* Reset the ADC IP */
N    outpw(REG_SYS_APBIPRST1, (1<<24));
X    *((volatile unsigned int *)((0xB0000000+0x068))) = (1<<24);
N    outpw(REG_SYS_APBIPRST1, 0);
X    *((volatile unsigned int *)((0xB0000000+0x068))) = 0;
N
N    /* ADC Engine Clock is set to freq Khz */
N    if(freqKhz>3000) freqKhz=3000;
N    if(freqKhz<1000) freqKhz=1000;
N    div=12000/freqKhz;
N    outpw(REG_CLK_DIVCTL7, inpw(REG_CLK_DIVCTL7) & ~((0x3<<19)|(0x7<<16)|(0xFFul<<24)));
X    *((volatile unsigned int *)((0xB0000200+0x3C))) = (*((volatile unsigned int *)((0xB0000200+0x3C)))) & ~((0x3<<19)|(0x7<<16)|(0xFFul<<24));
N    outpw(REG_CLK_DIVCTL7, (0<<19)|(0<<16)|((div-1)<<24));
X    *((volatile unsigned int *)((0xB0000200+0x3C))) = (0<<19)|(0<<16)|((div-1)<<24);
N
N    /* Enable ADC Power */
N    outpw(REG_ADC_CTL, ADC_CTL_ADEN);
X    *((volatile unsigned int *)((0xB800A000+0x000))) = 0x00000001;
N
N    /* Enable ADC to high speed mode */
N    outpw(REG_ADC_CONF, inpw(REG_ADC_CONF)|ADC_CONF_HSPEED);
X    *((volatile unsigned int *)((0xB800A000+0x004))) = (*((volatile unsigned int *)((0xB800A000+0x004))))|(1<<22);
N
N    /* Set interrupt */
N    sysSetInterruptType(ADC_IRQn, HIGH_LEVEL_SENSITIVE);
X    sysSetInterruptType(ADC_IRQn, 0x40);
N    sysInstallISR(IRQ_LEVEL_7, ADC_IRQn, (PVOID)adcISR);
X    sysInstallISR(7, ADC_IRQn, (PVOID)adcISR);
N    sysSetLocalInterrupt(ENABLE_IRQ);                            /* enable CPSR I bit */
X    sysSetLocalInterrupt(0x7F);                             
N    sysEnableInterrupt(ADC_IRQn);
N
N    /* Init the FIFO buffer */
N    adcHandler.fifoHeadX = 0;
N    adcHandler.fifoTailX = 0;
N    adcHandler.fifoLengthX = 0;
N    adcHandler.fifoHeadY = 0;
N    adcHandler.fifoTailY = 0;
N    adcHandler.fifoLengthY = 0;
N    adcHandler.fifoHeadZ1 = 0;
N    adcHandler.fifoTailZ1 = 0;
N    adcHandler.fifoLengthZ1 = 0;
N    adcHandler.fifoHeadZ2 = 0;
N    adcHandler.fifoTailZ2 = 0;
N    adcHandler.fifoLengthZ2 = 0;
N
N    adcHandler.touch_callback=NULL;
X    adcHandler.touch_callback=(0);
N    adcHandler.touch_userData=NULL;
X    adcHandler.touch_userData=(0);
N
N    adcHandler.touchz_callback=NULL;
X    adcHandler.touchz_callback=(0);
N    adcHandler.touchz_userData=NULL;
X    adcHandler.touchz_userData=(0);
N
N    adcHandler.pendown_callback=NULL;
X    adcHandler.pendown_callback=(0);
N    adcHandler.pendown_userData=NULL;
X    adcHandler.pendown_userData=(0);
N
N    adcHandler.penup_callback=NULL;
X    adcHandler.penup_callback=(0);
N    adcHandler.penup_userData=NULL;
X    adcHandler.penup_userData=(0);
N
N    adcHandler.keypad_conv_callback=NULL;
