N/**
N * @brief       The read touch xy data of ADC.
N *
N * @param[out]  bufX     Data buffer for x-position data
N * @param[out]  bufY     Data buffer for y-position data
N * @param[in]   dataCnt  The length of ADC x- and y-position data from FIFO
N *
N * @return      Data count actually
N *
N * @details     This function is used to read touch x-position data and touch y-position data .
N */
NINT adcReadXY(INT16 *bufX, INT16 *bufY,int dataCnt)
N{
N    INT32 i;
N    volatile INT16 *fifoX, *fifoY;
N    volatile INT32 headX, headY;
N    volatile INT32 fifoLengthX, readLengthX;
N    volatile INT32 fifoLengthY, readLengthY;
N
N    fifoX = adcHandler.fifoX;
N    fifoY = adcHandler.fifoY;
N    headX = adcHandler.fifoHeadX;
N    headY = adcHandler.fifoHeadY;
N    fifoLengthX = adcHandler.fifoLengthX;
N    fifoLengthY = adcHandler.fifoLengthY;
N
N    readLengthX = 0;
N    readLengthY = 0;
N
N    for (i = 0; i < dataCnt; i++) {
N        if (fifoLengthX > readLengthX) {
N            bufX[i] = fifoX[headX];
N            readLengthX++;
N            headX++;
N            if (headX >= ADC_FIFO_LENGTH) {
X            if (headX >= 128) {
N                headX = 0;
N            }
N        } else {
N            /* FIFO is empty, just return */
N            break;
N        }
N    }
N
N    for (i = 0; i < dataCnt; i++) {
N        if (fifoLengthY > readLengthY) {
N            bufY[i] = fifoY[headY];
N            readLengthY++;
N            headY++;
N            if (headY >= ADC_FIFO_LENGTH) {
X            if (headY >= 128) {
N                headY = 0;
N            }
N        } else {
N            /* FIFO is empty, just return */
N            break;
N        }
N    }
N
N    /* Update FIFO information */
N    adcHandler.fifoHeadX = headX;
N    adcHandler.fifoLengthX -= readLengthX;
N    adcHandler.fifoHeadY = headY;
N    adcHandler.fifoLengthY -= readLengthY;
N    return i;
N}
