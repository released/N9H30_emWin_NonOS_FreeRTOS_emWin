    spiIoctl(0, 0, 0, 0);
    while(spiGetBusyStatus(0));
    u8RxData[0] = spiRead(0, 0);

    spiWrite(0, 0, 0x00);
    spiIoctl(0, 0, 0, 0);
    while(spiGetBusyStatus(0));
    u8RxData[1] = spiRead(0, 0);

    
    spiIoctl(0, 10, 0, 0);

    return ( (u8RxData[0]<<8) | u8RxData[1] );
}
