Nstatic void prvSetupTimerInterrupt( void )
N{
Nuint32_t ulCompareMatch;
N
N	// enable the Timer 1 clock	
N  outpw(REG_CLK_PCLKEN0, inpw(REG_CLK_PCLKEN0)| 0x200);
X  *((volatile unsigned int *)((0xB0000200+0x18))) = (*((volatile unsigned int *)((0xB0000200+0x18))))| 0x200;
N  outp32(REG_SYS_APBIPRST0, inp32(REG_SYS_APBIPRST0) | BIT9);
X  *((volatile unsigned int *)((0xB0000000+0x064))) = (*((volatile unsigned int *)((0xB0000000+0x064)))) | (0x00000200);
N	outp32(REG_SYS_APBIPRST0, inp32(REG_SYS_APBIPRST0) & ~BIT9);
X	*((volatile unsigned int *)((0xB0000000+0x064))) = (*((volatile unsigned int *)((0xB0000000+0x064)))) & ~(0x00000200);
N
N	/* Calculate the match value required for our wanted tick rate. */
N	//ulCompareMatch = EXTERNAL_CRYSTAL_CLOCK / configTICK_RATE_HZ;
N	ulCompareMatch = 12000000 / configTICK_RATE_HZ;
X	ulCompareMatch = 12000000 / ( ( TickType_t ) 1000 );
N
N	/* Protect against divide by zero.  Using an if() statement still results
N	in a warning - hence the #if. */
N	#if portPRESCALE_VALUE != 0
S	{
S		ulCompareMatch /= ( portPRESCALE_VALUE + 1 );
S	}
N	#endif
N
N	/* Setup the AIC for the timer. */
N	#if 0
S	/* The ISR installed depends on whether the preemptive or cooperative
S	scheduler is being used. */
S	#if configUSE_PREEMPTION == 1
S	{	
S		sysInstallISR(IRQ_LEVEL_1, TMR1_IRQn, (PVOID)vPreemptiveTick);
S	}
S	#else
S	{
S		sysInstallISR(IRQ_LEVEL_1, TMR1_IRQn, (PVOID)vNonPreemptiveTick);
S	}
S	#endif
N	#endif
N	
N	_sys_bIsAICInitial = TRUE;
X	_sys_bIsAICInitial = (1);
N	
N#if configUSE_PREEMPTION == 1	
X#if 1 == 1	
N	sysInstallIrqHandler(vPreemptiveTick);
N#endif	
N
N	sysEnableInterrupt(TMR1_IRQn);
N
N	outpw(REG_TMR1_TICR, ulCompareMatch);
X	*((volatile unsigned int *)((0xB8001010+0x04))) = ulCompareMatch;
N	outpw(REG_TMR1_TCSR, (inpw(REG_TMR1_TCSR) & 0x81FFFF00) | (0xD << 27));		// 0xC means CEN and IE were enable	
X	*((volatile unsigned int *)((0xB8001010+0x00))) = ((*((volatile unsigned int *)((0xB8001010+0x00)))) & 0x81FFFF00) | (0xD << 27);		
N	
N}
