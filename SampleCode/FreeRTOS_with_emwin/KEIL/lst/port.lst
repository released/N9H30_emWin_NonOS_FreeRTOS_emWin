N    if (_mISNR != 0)
N        if (_mIPER == _mISNR)
N            (*sysIrqHandlerTable[_mIPER])();
N		
N    outpw(REG_AIC_EOSCR, 1);
X    *((volatile unsigned int *)((0xB8002000+0x150))) = 1;
N}
