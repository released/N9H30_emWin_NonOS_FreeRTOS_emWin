NBaseType_t xPortStartScheduler( void )
N{
N	/* Start the timer that generates the tick ISR. */
N	prvSetupTimerInterrupt();
N
N	/* Start the first task.  This is done from portISR.c as ARM mode must be
N	used. */
N	vPortStartFirstTask();
N
N	/* Should not get here! */
N	return 0;
N}
