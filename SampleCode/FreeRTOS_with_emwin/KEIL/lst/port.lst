N/*-----------------------------------------------------------*/
N
N/* 
N * See header file for description. 
N */
NStackType_t *pxPortInitialiseStack( StackType_t *pxTopOfStack, TaskFunction_t pxCode, void *pvParameters )
N{
NStackType_t *pxOriginalTOS;
N
N	/* Setup the initial stack of the task.  The stack is set exactly as 
N	expected by the portRESTORE_CONTEXT() macro.
N
N	Remember where the top of the (simulated) stack is before we place 
N	anything on it. */
N	pxOriginalTOS = pxTopOfStack;
N	
N	/* To ensure asserts in tasks.c don't fail, although in this case the assert
N	is not really required. */
N	pxTopOfStack--;
N
N	/* First on the stack is the return address - which in this case is the
N	start of the task.  The offset is added to make the return address appear
N	as it would within an IRQ ISR. */
N	*pxTopOfStack = ( StackType_t ) pxCode + portINSTRUCTION_SIZE;		
X	*pxTopOfStack = ( StackType_t ) pxCode + ( ( StackType_t ) 4 );		
N	pxTopOfStack--;
N
N	*pxTopOfStack = ( StackType_t ) 0xaaaaaaaa;	/* R14 */
N	pxTopOfStack--;	
N	*pxTopOfStack = ( StackType_t ) pxOriginalTOS; /* Stack used when task starts goes in R13. */
N	pxTopOfStack--;
N	*pxTopOfStack = ( StackType_t ) 0x12121212;	/* R12 */
N	pxTopOfStack--;	
N	*pxTopOfStack = ( StackType_t ) 0x11111111;	/* R11 */
N	pxTopOfStack--;	
N	*pxTopOfStack = ( StackType_t ) 0x10101010;	/* R10 */
N	pxTopOfStack--;	
N	*pxTopOfStack = ( StackType_t ) 0x09090909;	/* R9 */
N	pxTopOfStack--;	
N	*pxTopOfStack = ( StackType_t ) 0x08080808;	/* R8 */
N	pxTopOfStack--;	
N	*pxTopOfStack = ( StackType_t ) 0x07070707;	/* R7 */
N	pxTopOfStack--;	
N	*pxTopOfStack = ( StackType_t ) 0x06060606;	/* R6 */
N	pxTopOfStack--;	
N	*pxTopOfStack = ( StackType_t ) 0x05050505;	/* R5 */
N	pxTopOfStack--;	
N	*pxTopOfStack = ( StackType_t ) 0x04040404;	/* R4 */
N	pxTopOfStack--;	
N	*pxTopOfStack = ( StackType_t ) 0x03030303;	/* R3 */
N	pxTopOfStack--;	
N	*pxTopOfStack = ( StackType_t ) 0x02020202;	/* R2 */
N	pxTopOfStack--;	
N	*pxTopOfStack = ( StackType_t ) 0x01010101;	/* R1 */
N	pxTopOfStack--;	
N	*pxTopOfStack = ( StackType_t ) pvParameters; /* R0 */
N	pxTopOfStack--;
N
N	/* The last thing onto the stack is the status register, which is set for
N	system mode, with interrupts enabled. */
N	*pxTopOfStack = ( StackType_t ) portINITIAL_SPSR;
X	*pxTopOfStack = ( StackType_t ) ( ( StackType_t ) 0x1f );
N
N	if( ( ( uint32_t ) pxCode & 0x01UL ) != 0x00UL )
N	{
N		/* We want the task to start in thumb mode. */
N		*pxTopOfStack |= portTHUMB_MODE_BIT;
X		*pxTopOfStack |= ( ( StackType_t ) 0x20 );
N	}
N
N	pxTopOfStack--;
N
N	/* The code generated by the Keil compiler does not maintain separate
N	stack and frame pointers. The portENTER_CRITICAL macro cannot therefore
N	use the stack as per other ports.  Instead a variable is used to keep
N	track of the critical section nesting.  This variable has to be stored
N	as part of the task context and is initially set to zero. */
N	*pxTopOfStack = portNO_CRITICAL_SECTION_NESTING;
X	*pxTopOfStack = ( ( StackType_t ) 0 );
N
N	return pxTopOfStack;
N}
