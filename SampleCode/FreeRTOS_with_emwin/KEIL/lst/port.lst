Nvoid vPortEnterCritical( void )
N{
N	/* Disable interrupts as per portDISABLE_INTERRUPTS(); 							*/
N	__disable_irq();
N
N	/* Now interrupts are disabled ulCriticalNesting can be accessed 
N	directly.  Increment ulCriticalNesting to keep a count of how many times
N	portENTER_CRITICAL() has been called. */
N	ulCriticalNesting++;
N}
N/*-----------------------------------------------------------*/
N
Nvoid vPortExitCritical( void )
N{
N	if( ulCriticalNesting > portNO_CRITICAL_NESTING )
X	if( ulCriticalNesting > ( ( uint32_t ) 0 ) )
N	{
N		/* Decrement the nesting count as we are leaving a critical section. */
N		ulCriticalNesting--;
N
N		/* If the nesting level has reached zero then interrupts should be
N		re-enabled. */
N		if( ulCriticalNesting == portNO_CRITICAL_NESTING )
X		if( ulCriticalNesting == ( ( uint32_t ) 0 ) )
N		{
N			/* Enable interrupts as per portEXIT_CRITICAL(). */
N			__enable_irq();
N		}
N	}
N}
